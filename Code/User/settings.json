{
  "cSpell.diagnosticLevel": "Hint",
  "editor.codeLensFontFamily": "'Hack Nerd Font', 'monospace'",
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "'Hack Nerd Font', monospace",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.rename.enablePreview": false,
  "editor.rulers": [
    {
      "column": 120,
      "color": "#424242"
    }
  ],
  "editor.stickyScroll.enabled": true,
  "editor.stickyTabStops": true,
  "editor.wordWrapColumn": 120,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": false,
  "files.simpleDialog.enable": true,
  "go.lintFlags": ["--fast"],
  "go.lintTool": "golangci-lint",
  "redhat.telemetry.enabled": false,
  "shellcheck.run": "onSave",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "'Hack Nerd Font', 'monospace'",
  "terminal.integrated.ignoreProcessNames": ["bash", "zsh"],
  "terminal.integrated.localEchoExcludePrograms": ["vim", "vi", "nano"],
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "window.menuBarVisibility": "toggle",
  "workbench.editor.navigationScope": "editorGroup",
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.startupEditor": "none"
}
